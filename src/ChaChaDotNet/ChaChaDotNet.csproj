<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Version>1.0.0</Version>
        <Authors>Samuel Lucas</Authors>
        <Description>A .NET implementation of ChaCha8, ChaCha12, and ChaCha20.</Description>
        <Copyright>Copyright (c) 2022 Samuel Lucas</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/samuel-lucas6/ChaCha.NET</PackageProjectUrl>
        <RepositoryUrl>https://github.com/samuel-lucas6/ChaCha.NET</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>chacha8 chacha12 chacha20 ietf</PackageTags>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>

</Project>
